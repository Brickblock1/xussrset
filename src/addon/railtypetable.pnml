 /// railtypetable

// Список меток типов рельсов
railtypetable {
// автономная
  RAIL: [RLA0, RLA1, RLA2, RLA3,
         SAAN, SABN, SACN, SADN, SAEN,
         RAIL],
// переменного тока 25 кВ 50 Гц
  ELAC: [ERA1, ERA2, ERA3, ERA4,
         SAAA, SABA, SACA, SADA, SAEA,
         ELRL, RAIL],
// переменного тока 25 кВ 50 Гц
  ELac: [ERa0, ERa1, ERa2, ERa3, ERa4,
         SAAa, SABa, SACa, SADa, SAEa,
         SAAA, SABA, SACA, SADA, SAEA,
         ELRL, RAIL],
// постоянного тока 3 кВ
  ELDC: [ERD1, ERD2, ERD3,
         SAAD, SABD, SACD, SADD, SAED,
         ELRL, RAIL],
// постоянного тока 1,5 кВ
  ELdc: [ERd0, ERd1, ERd2, ERd3,
         SAAd, SABd, SACd, SADd, SAEd,
         SAAD, SABD, SACD, SADD, SAED,
         ELRL, RAIL],
// 2D = D + d (1,5 кВ + 3 кВ)
  EL2D: [ER2D,
         SAAD, SABD, SACD, SADD, SAED,
         SAAd, SABd, SACd, SADd, SAEd,
         ELRL, RAIL],
// 2S = A + D (3 кВ + 25 кВ)
  EL2S: [ER2S,
         SAAA, SABA, SACA, SADA, SAEA,
         SAAD, SABD, SACD, SADD, SAED,
         ELRL, RAIL],
// 2s = A + d (TGV-PSE)
  EL2s: [ER2s,
         SAAA, SABA, SACA, SADA, SAEA,
         SAAd, SABd, SACd, SADd, SAEd,
         SAAD, SABD, SACD, SADD, SAED,
         ELRL, RAIL],
// 3D = A + D + d (ETR500)
  EL3D: [ER3D,
         SAAA, SABA, SACA, SADA, SAEA,
         SAAD, SABD, SACD, SADD, SAED,
         SAAd, SABd, SACd, SADd, SAEd,
         ELRL, RAIL],
// 3a = A + a + d (TGV-POS)
  EL3a: [ER3a,
         SAAA, SABA, SACA, SADA, SAEA,
         SAAa, SABa, SACa, SADa, SAEa,
         SAAd, SABd, SACd, SADd, SAEd,
         SAAD, SABD, SACD, SADD, SAED,
         ELRL, RAIL],
// 4S = A + D + a + d (ETR575)
  EL4S: [ER4S,
         SAAA, SABA, SACA, SADA, SAEA,
         SAAa, SABa, SACa, SADa, SAEa,
         SAAD, SABD, SACD, SADD, SAED,
         SAAd, SABd, SACd, SADd, SAEd,
         ELRL, RAIL]
}
