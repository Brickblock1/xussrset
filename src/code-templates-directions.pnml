switch (FEAT_TRAINS, SELF, name##suffix##_sprites_start_sp,     \
[  set_offset_to(1),                                            \
   (prev_vehicle_randombits() % (p1 + p2)) < p1])               \
{ 1: name##suffix##_sprites_left;                               \
     name##suffix##_sprites_right; }                            \

