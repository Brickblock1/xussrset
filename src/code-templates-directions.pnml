#define engine_RNDany_direction_template(name, p1, p2)  \
switch (FEAT_TRAINS, SELF, name##_sprites_start_sp,     \
[  set_offset_to(1),                                            \
   (prev_vehicle_randombits() % (p1 + p2)) < p1])               \
{ 1: name##_sprites_left;                               \
     name##_sprites_right; }                            \

