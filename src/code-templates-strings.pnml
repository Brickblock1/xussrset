 /// code definition

#define hint_change_longnames(name)                                    \
switch (FEAT_TRAINS, SELF, name##_additional_text, enable_long_names)  \
{ 1: name##_long_additional_text;                                      \
     name##_short_additional_text; }                                   \

// шаблон выбора по году
#define hint_change_after(name, year)                                     \
switch (FEAT_TRAINS, SELF, name##_additional_text, current_year >= year)  \
{ 0: name##_b##year##_additional_text;                                    \
     name##_a##year##_additional_text; }                                  \

#define hint_change_after2(name, year, year2)       \
switch (FEAT_TRAINS, SELF, name##_additional_text,  \
  (current_year >= year) +                          \
  (current_year >= year2))                          \
{ 0: name##_b##year##_additional_text;              \
  1: name##_a##year##_additional_text;              \
     name##_a##year2##_additional_text; }           \

#define hint_change_after3(name, year, year2, year3)  \
switch (FEAT_TRAINS, SELF, name##_additional_text,    \
  (current_year >= year) +                            \
  (current_year >= year2) +                           \
  (current_year >= year3))                            \
{ 0: name##_b##year##_additional_text;                \
  1: name##_a##year##_additional_text;                \
  2: name##_a##year2##_additional_text;               \
     name##_a##year3##_additional_text; }             \

#define hint_change_after4(name, year, year2, year3, year4)  \
switch (FEAT_TRAINS, SELF, name##_additional_text,           \
  (current_year >= year) +                                   \
  (current_year >= year2) +                                  \
  (current_year >= year3) +                                  \
  (current_year >= year4))                                   \
{ 0: name##_b##year##_additional_text;                       \
  1: name##_a##year##_additional_text;                       \
  2: name##_a##year2##_additional_text;                      \
  3: name##_a##year3##_additional_text;                      \
     name##_a##year4##_additional_text; }                    \

#define hint_change_after5(name, year, year2, year3, year4, year5)  \
switch (FEAT_TRAINS, SELF, name##_additional_text,                  \
  (current_year >= year) +                                          \
  (current_year >= year2) +                                         \
  (current_year >= year3) +                                         \
  (current_year >= year4) +                                         \
  (current_year >= year5))                                          \
{ 0: name##_b##year##_additional_text;                              \
  1: name##_a##year##_additional_text;                              \
  2: name##_a##year2##_additional_text;                             \
  3: name##_a##year3##_additional_text;                             \
  4: name##_a##year4##_additional_text;                             \
     name##_a##year5##_additional_text; }                           \

#define hint_change_after6(name, year, year2, year3, year4, year5, year6)  \
