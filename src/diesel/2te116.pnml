  (current_year >= 1973) +
  (current_year >= 1977) +
  (current_year >= 1979) +
  (current_year >= 1982) +
  (current_year >= 1984) +
  (current_year >= 1991) +
  (current_year >= 1994) +
  (current_year >= 1997) +
  (current_year >= 2005) +
  (current_year >= 2007))
{
  1: return 76;
  2: return 81;
  3: return 83;
  4: return 86;
  5: return 89;
  6: return 98;
  7: return 102;
  8: return 120;
  9: return 170;
  10: return 240;
  return 74;
}

