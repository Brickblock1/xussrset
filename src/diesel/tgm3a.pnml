    vehicle_dates(1961, 1970, 20, 10, 6, 8)
    vehicle_cargo()
    vehicle_diesel(500, 68, 220.0)
  }
  graphics {
    default: tgm3a_sprites;
    purchase: tgm3_purchase_sprites;
    articulated_part: articulated_part_dummy1;
    can_attach_wagon: any_can_attach_wagon;
    start_stop: any_start_stop;
    additional_text: return string(STR_PURCHASE_HINT_ENGINE,
                                   string(STR_PURCHASE_HINT_ENGINE_TYPE_SHUNTING_DIESEL),
                                   string(STR_PURCHASE_HINT_SECTIONS_1_2),
                                   string(STR_PURCHASE_HINT_FROMTO, 1961, 1970),
                                   string(STR_PURCHASE_HINT_FACTORY_LYUDINOVSK));
    speed: tgm3a_speed;
    running_cost_factor: tgm3a_running_cost_factor;
    bitmask_vehicle_info: tgm3a_bitmask_vehicle_info;
    cargo_capacity: return 0;
    create_effect: tgm3a_create_effect;
    cargo_subtype_text: tgm3_cargo_subtype_text;
    colour_mapping: tgm3_colour_mapping;
    purchase_speed: return 62;
  }
}

