switch (FEAT_TRAINS, PARENT, ed9t_m_visual_effect_and_powered,
  vehicle_type_id == ed9t_h)
{
  1: ed9t_m_visual_effect_and_powered_direction_template;
     return disable_visual_effect_and_powered();
}

// Даём тягу, только если локомотив - ed9t_h
switch (FEAT_TRAINS, PARENT, ed9t_m_power,
  vehicle_type_id == ed9t_h)
{
  1: return get_power_in_cb(1066); 
     return 0;
}

// Допускается эксплуатация с пассажирами только если голова - ed9t_h
switch (FEAT_TRAINS, PARENT, ed9t_h_cargo_capacity,
  (vehicle_type_id == ed9t_h))
{
  1: return 80;
     return 0;
}

switch (FEAT_TRAINS, PARENT, ed9t_m_cargo_capacity,
  (vehicle_type_id == ed9t_h))
{
  1: return 112;
     return 0;
}

switch (FEAT_TRAINS, PARENT, ed9t_c_cargo_capacity,
  (vehicle_type_id == ed9t_h))
{
  1: return 116;
     return 0;
}

