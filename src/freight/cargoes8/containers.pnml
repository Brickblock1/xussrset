#define container_sprite_layout_template1_1(suffix, x, y)                            \
spriteset (container_##suffix##_01_sprites_l, IMAGEFILE) { s8_template(x, y, 0) }    \
spriteset (container_##suffix##_01_sprites_r, IMAGEFILE) { s8_r_template(x, y, 0) }  \
switch (FEAT_TRAINS, SELF, container_##suffix##_sprites_l, cargo_count)              \
{ 0: dummy_sprites; container_##suffix##_01_sprites_l; }                             \
