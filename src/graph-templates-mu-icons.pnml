[ STORE_TEMP(CB_FLAG_MORE_SPRITES | PALETTE_USE_DEFAULT, 0x100),                        \
  getbits(extra_callback_info1, 8, 8) > 0 ])                                            \
{ 1: name##_sprites_start_add0;                                                         \
     name##_direction_template; }                                                       \

