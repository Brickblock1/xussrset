                                              vehid11, vehid12, icon)                   \
switch (FEAT_TRAINS, SELF, name##_sprites_start_add2,                                   \
  (position_in_consist_from_end == 1) )                                                 \
{ 1: name##_next_can_attach_wagon_template;                                             \
     dummy_sprites; }                                                                   \
