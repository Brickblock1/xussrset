 /// railtypetable

// Список меток типов рельсов
railtypetable {
  RAIL,
  ELRL,
// автономная
  SAAN: [BAAN, BABN, BACN, BADN, BAEN, SAAN, SABN, SACN, SADN, SAEN, RLA0],
  SBAN: [BBAN, BBBN, BBCN, BBDN, BBEN, SBAN, SBBN, SBCN, SBDN, SBEN, RLA1],
  SCAN: [BCAN, BCBN, BCCN, BCDN, BCEN, SCAN, SCBN, SCCN, SCDN, SCEN, RLA2],
  SDAN: [BDAN, BDBN, BDCN, BDDN, BDEN, SDAN, SDBN, SDCN, SDDN, SDEN, RLA3],
// переменного тока 25 кВ 50 Гц
  SAAA: [BAAA, BABA, BACA, BADA, BAEA, SAAA, SABA, SACA, SADA, SAEA, ERA0],
  SBAA: [BBAA, BBBA, BBCA, BBDA, BBEA, SBAA, SBBA, SBCA, SBDA, SBEA, ERA1],
  SCAA: [BCAA, BCBA, BCCA, BCDA, BCEA, SCAA, SCBA, SCCA, SCDA, SCEA, ERA2],
  SDAA: [BDAA, BDBA, BDCA, BDDA, BDEA, SDAA, SDBA, SDCA, SDDA, SDEA, ERA3],
  SEAA: [BEAA, BEBA, BECA, BEDA, BEEA, SEAA, SEBA, SECA, SEDA, SEEA, ERA4],
// переменного тока 25 кВ 50 Гц
  SAAa: [BAAa, BABa, BACa, BADa, BAEa, SAAa, SABa, SACa, SADa, SAEa, ERa0],
  SBAa: [BBAa, BBBa, BBCa, BBDa, BBEa, SBAa, SBBa, SBCa, SBDa, SBEa, ERa1],
  SCAa: [BCAa, BCBa, BCCa, BCDa, BCEa, SCAa, SCBa, SCCa, SCDa, SCEa, ERa2],
  SDAa: [BDAa, BDBa, BDCa, BDDa, BDEa, SDAa, SDBa, SDCa, SDDa, SDEa, ERa3],
  SEAa: [BEAa, BEBa, BECa, BEDa, BEEa, SEAa, SEBa, SECa, SEDa, SEEa, ERa4],
// постоянного тока 3 кВ
  SAAD: [BAAD, BABD, BACD, BADD, BAED, SAAD, SABD, SACD, SADD, SAED, ERD0],
  SBAD: [BBAD, BBBD, BBCD, BBDD, BBED, SBAD, SBBD, SBCD, SBDD, SBED, ERD1],
  SCAD: [BCAD, BCBD, BCCD, BCDD, BCED, SCAD, SCBD, SCCD, SCDD, SCED, ERD2],
  SDAD: [BDAD, BDBD, BDCD, BDDD, BDED, SDAD, SDBD, SDCD, SDDD, SDED, ERD3],
// постоянного тока 1,5 кВ
  SAAd: [BAAd, BABd, BACd, BADd, BAEd, SAAd, SABd, SACd, SADd, SAEd, ERd0],
  SBAd: [BBAd, BBBd, BBCd, BBDd, BBEd, SBAd, SBBd, SBCd, SBDd, SBEd, ERd1],
  SCAd: [BCAd, BCBd, BCCd, BCDd, BCEd, SCAd, SCBd, SCCd, SCDd, SCEd, ERd2],
  SDAd: [BDAd, BDBd, BDCd, BDDd, BDEd, SDAd, SDBd, SDCd, SDDd, SDEd, ERd3],
// 2S = A + D (3 кВ + 25 кВ)
  ER2S,
// 2D = D + d (1,5 кВ + 3 кВ)
  ER2D,
// 4S = A + D + a + d (ETR575)
  ER4S,
// 2s = A + d (TGV-PSE)
  ER2s,
// 3D = A + D + d (ETR500)
  ER3D,
// 3a = A + a + d (TGV-POS)
  ER3a,
// Genegdl electric
  SAAE: [BAAE, BABE, BACE, BADE, BAEE, SAAE, SABE, SACE, SADE, SAEE, ER2S],
  SBAE: [BBAE, BBBE, BBCE, BBDE, BBEE, SBAE, SBBE, SBCE, SBDE, SBEE, ER2S],
  SCAE: [BCAE, BCBE, BCCE, BCDE, BCEE, SCAE, SCBE, SCCE, SCDE, SCEE, ER2S],
  SDAE: [BDAE, BDBE, BDCE, BDDE, BDEE, SDAE, SDBE, SDCE, SDDE, SDEE, ER2S],
  SEAE: [BEAE, BEBE, BECE, BEDE, BEEE, SEAE, SEBE, SECE, SEDE, SEEE, ER2S],
}

RAILS_NORMAL = (railtype_available("SAAN") ? SAAN : RAIL);

RAILS_ER25AC = (railtype_available("SAAA") ? SAAA : railtype_available("SAAE") ? SAAE : ELRL);
RAILS_ER15AC = (railtype_available("SAAa") ? SAAa : railtype_available("SAAA") ? SAAA : railtype_available("SAAE") ? SAAE : ELRL);
RAILS_ER3DC  = (railtype_available("SAAD") ? SAAD : railtype_available("SAAE") ? SAAE : ELRL);
RAILS_ER15DC = (railtype_available("SAAd") ? SAAd : railtype_available("SAAD") ? SAAD : railtype_available("SAAE") ? SAAE : ELRL);

RAILS_ER2S = (railtype_available("ER2S") ? ER2S : railtype_available("SAAE") ? SAAE : ELRL);

RAILS_ER2D = (railtype_available("ER2D") ? ER2D : railtype_available("SAAD") ? SAAD : railtype_available("SAAd") ? SAAd : railtype_available("SAAE") ? SAAE : ELRL);
RAILS_ER2s = (railtype_available("ER2s") ? ER2s : railtype_available("SAAA") ? SAAA : railtype_available("SAAd") ? SAAd : railtype_available("SAAE") ? SAAE : ELRL);
RAILS_ER3D = (railtype_available("ER3D") ? ER3D : railtype_available("SAAA") ? SAAA : railtype_available("SAAD") ? SAAD : railtype_available("SAAd") ? SAAd : railtype_available("SAAE") ? SAAE : ELRL);
RAILS_ER3a = (railtype_available("ER3a") ? ER3a : railtype_available("SAAA") ? SAAA : railtype_available("SAAa") ? SAAa : railtype_available("SAAd") ? SAAd : railtype_available("SAAE") ? SAAE : ELRL);

RAILS_ER4S = (railtype_available("ER4S") ? ER4S : railtype_available("SAAE") ? SAAE : ELRL);
